#include <stdio.h>

 struct presone{
       char nom[10] ;
       char prenom[10] ;
       int age ;
   }per;
   
int main() {
    // Rdmplissage
     struct presone pre;
    printf("enter le nom\t");
    scanf(" %s",per.nom); 
    printf("enter le nom\t");
    scanf("%s",per.prenom);
     printf("enter le nom\t");
    scanf("%d",&per.age);
    //afichage
    printf("le nom est %s , le prenom %s ,est le age %d",per.nom,per.prenom,per.age);
    return 0;
}
